<h1>Sign In</h1>

<div class="center">
  <%= form_tag(sessions_path) do %>

    <div class="form-group">
      <%= label_tag :username%>
      <br>
      <%= text_field_tag :username%>
    </div>
    <div class="form-group">
      <%= label_tag :password%>
      <br>
      <%= password_field_tag :password%>
    </div>
    <div class="form-group">
      <%= submit_tag "Sign In" %>
    </div>

  <% end %>
</div>
